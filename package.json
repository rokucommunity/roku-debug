{
    "name": "roku-debug",
    "version": "0.8.4",
    "description": "Debug adapter for Roku application development using Node.js",
    "main": "dist/index.js",
    "scripts": {
        "build": "rimraf dist && tsc",
        "preversion": "npm run build && npm run lint && npm run test",
        "lint": "eslint \"src/**\"",
        "test": "nyc mocha \"src/**/*spec.ts\" --full-trace",
        "test:nocover": "ts-mocha \"src/**/*.spec.ts\" --full-trace",
        "publish-coverage": "nyc report --reporter=text-lcov | coveralls"
    },
    "typings": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/rokucommunity/roku-debug"
    },
    "author": "",
    "license": "ISC",
    "nyc": {
        "include": [
            "src/**/!(*.spec).ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true,
        "check-coverage": false,
        "lines": 100,
        "statements": 100,
        "functions": 100,
        "branches": 100
    },
    "devDependencies": {
        "@types/archiver": "^2.1.2",
        "@types/chai": "^4.2.3",
        "@types/denodeify": "^1.2.31",
        "@types/fs-extra": "^5.0.1",
        "@types/mocha": "^5.2.5",
        "@types/node": "^12.12.28",
        "@types/request": "^2.47.0",
        "@types/sinon": "^7.0.6",
        "@types/vscode": "^1.30.0",
        "chai": "^4.2.0",
        "chai-files": "^1.4.0",
        "coveralls": "^3.0.0",
        "eslint": "^7.21.0",
        "eslint-plugin-github": "^4.1.1",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "get-port": "^5.1.1",
        "mocha": "^8.2.1",
        "mock-fs": "^4.10.3",
        "nyc": "^14.1.1",
        "p-defer": "^3.0.0",
        "rimraf": "^2.6.2",
        "rmfr": "^2.0.0",
        "rxjs": "^6.5.4",
        "sinon": "^7.2.3",
        "source-map-support": "^0.5.13",
        "testdouble": "^3.5.2",
        "ts-mocha": "^6.0.0",
        "ts-node": "^8.4.1",
        "typescript": "^3.7.2",
        "vscode-debugadapter-testsupport": "1.33.0"
    },
    "dependencies": {
        "brighterscript": "^0.41.1",
        "dateformat": "^3.0.3",
        "eol": "^0.9.1",
        "find-in-files": "^0.5.0",
        "fs-extra": "^8.1.0",
        "natural-orderby": "^2.0.3",
        "replace-in-file": "^5.0.2",
        "replace-last": "^1.2.6",
        "roku-deploy": "^3.4.1",
        "semver": "^7.1.3",
        "serialize-error": "^6.0.0",
        "smart-buffer": "^4.1.0",
        "source-map": "^0.7.3",
        "telnet-client": "^1.4.9",
        "vscode-debugadapter": "1.33.0",
        "vscode-debugprotocol": "1.33.0"
    }
}
